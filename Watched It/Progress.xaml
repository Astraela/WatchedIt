<Window x:Class="Watched_It.Progress"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Watched_It"
        mc:Ignorable="d"
        Title="Change Progress" Height="122.8" Width="320" Background="#FF1D1D1D" Icon="pack://siteoforigin:,,,/WatchedIt.ico">
    <Grid>
        <TextBox x:Name="ProgressBox" HorizontalAlignment="Left" Height="30" Margin="8,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="290" FontSize="20"/>
        <Button x:Name="button" Content="Update" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Width="288" Height="34" FontSize="20" Click="button_Click" />

    </Grid>
</Window>
